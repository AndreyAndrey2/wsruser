<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.foods.FoodsFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/eat_1"
        android:layout_width="156dp"
        android:layout_height="212dp"
        android:layout_marginTop="60dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/bg_white_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <TextView
            android:id="@+id/tv1"
            android:layout_width="110dp"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="Veggie tomato mix"
            android:textColor="#000000"
            android:textSize="22dp"
            android:layout_marginTop="36dp"
            android:fontFamily="@font/nunito"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/price1"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="N1,900"
            android:textAlignment="center"
            android:textColor="@color/red"
            android:textSize="17dp"
            android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/food_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:src="@drawable/eat_1"
        app:layout_constraintBottom_toBottomOf="@+id/eat_1"
        app:layout_constraintEnd_toEndOf="@+id/eat_1"
        app:layout_constraintStart_toStartOf="@+id/eat_1"
        app:layout_constraintTop_toTopOf="@+id/eat_1" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/eat_2"
        android:layout_width="156dp"
        android:layout_height="212dp"
